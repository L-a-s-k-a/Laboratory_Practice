{
  "configurations": [
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "STM32F429xx"
      ],
      "compilerPath": "C:/tools/GCC/bin/arm-none-eabi-gcc.exe",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "gcc-arm",
      "compilerArgs": [
        "-mcpu=cortex-m4",
        "-mthumb"
      ]
    }
  ],
  "version": 4
}